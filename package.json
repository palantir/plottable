{
  "name": "plottable",
  "description": "A modular charting library built on D3",
  "version": "2.9.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/palantir/plottable.git"
  },
  "author": "Palantir Technologies",
  "main": "plottable-npm.js",
  "typings": "plottable-npm.d.ts",
  "scripts": {
    "build-src": "npm-run-all compile-typescript webpack-plottable webpack-plottable-npm",
    "build-test": "npm-run-all compile-typescript-test webpack-test",
    "check_version": "echo ${npm_package_version} $(npm view ${npm_package_name} version) | awk '{ if ($1 == $2) { exit 1 }}'",
    "compile-typescript": "tsc -p .",
    "compile-typescript-test": "tsc -p test/",
    "start": "grunt",
    "test": "grunt test-travis",
    "webpack-plottable": "webpack --config webpack.config.plottable.js",
    "webpack-plottable-npm": "webpack --config webpack.config.plottable-npm.js",
    "webpack-test": "webpack --config webpack.config.test.js"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.27",
    "@types/sinon": "^1.16.26",
    "chai": "2.0.0",
    "circle-github-bot": "0.3.0",
    "grunt": "~0.4.5",
    "grunt-blanket-mocha": "0.5.0",
    "grunt-bump": "0.7.0",
    "grunt-cli": "0.1.13",
    "grunt-contrib-clean": "0.7.0",
    "grunt-contrib-compress": "0.14.0",
    "grunt-contrib-concat": "0.5.1",
    "grunt-contrib-connect": "0.11.2",
    "grunt-contrib-uglify": "0.11.0",
    "grunt-contrib-watch": "0.6.1",
    "grunt-eslint": "17.3.1",
    "grunt-exec": "1.0.1",
    "grunt-git": "0.3.7",
    "grunt-jscs": "2.6.0",
    "grunt-mocha-phantomjs": "2.0.1",
    "grunt-parallelize": "1.1.3",
    "grunt-saucelabs": "8.6.2",
    "grunt-sed": "0.1.1",
    "grunt-shell": "1.1.2",
    "grunt-ts": "5.5.0",
    "grunt-tslint": "3.0.3",
    "grunt-umd": "2.3.5",
    "jquery": "2.1.0",
    "load-grunt-tasks": "3.4.0",
    "mocha": "2.2.5",
    "npm-run-all": "4.0.1",
    "requirejs": "2.1.18",
    "sinon": "1.16.1",
    "tslint": "3.15.1",
    "typescript": "2.0.6",
    "webpack": "2.2.1"
  },
  "dependencies": {
    "d3": "^3.5.16",
    "svg-typewriter": "1.0.2"
  }
}
