  },
  "homepage": "http://plottablejs.org",
  "repository": {
    "type": "git",
    "url": "git://github.com/palantir/plottable.git"
  },
  "devDependencies": {
    "chai": "2.0.0",
    "mocha": "2.2.5",
    "jQuery": "2.1.0",
    "jquery.simulate": "1.2.0",
    "requirejs": "2.1.18",
    "sinon": "1.16.1"
  }
