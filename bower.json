{
  "name": "plottable",
  "description": "A modular charting library built on D3",
  "version": "2.2.0",
  "main": [
    "plottable.js",
    "plottable.css"
  ],
  "typescript": {
    "definition": "plottable.d.ts"
  },
  "license": "MIT",
  "ignore": [
    "**/*",
    "!bower.json",
    "!plottable.js",
    "!plottable.css",
    "!plottable.min.js",
    "!plottable.d.ts"
  ],
  "keywords": [
    "plottable",
    "plottablejs",
    "plottable.js",
    "d3",
    "data viz",
