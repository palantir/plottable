{
  "name": "plottable",
  "description": "A modular charting library built on D3",
  "version": "3.0.0-beta.2",
  "main": [
    "plottable.js",
    "plottable.css"
  ],
  "typescript": {
    "definition": "plottable.d.ts"
  },
  "license": "MIT",
  "ignore": [
    "**/*",
    "!bower.json",
    "!plottable.js",
